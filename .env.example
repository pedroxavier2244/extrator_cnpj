# Copie este arquivo: cp .env.example .env

# --- Credenciais e conexao ---
POSTGRES_USER=TROCAR_USUARIO
POSTGRES_PASSWORD=TROCAR_SENHA_FORTE
DATABASE_URL=postgresql+psycopg2://TROCAR_USUARIO:TROCAR_SENHA@db:5432/cnpj

# --- Seguranca API ---
API_KEYS=TROCAR_CHAVE_1,TROCAR_CHAVE_2
CORS_ORIGINS=http://localhost:3000
ENVIRONMENT=production

# --- Caminhos ETL ---
RAW_DATA_PATH=data/raw
STAGING_PATH=data/staging
PROCESSED_PATH=data/processed
BATCH_SIZE=30000
ETL_HASH_ALGORITHM=sha256

# --- API ---
API_V1_PREFIX=/api/v1
APP_NAME=Sistema CNPJ
LOG_LEVEL=INFO

# --- Pool de conexoes ---
DB_POOL_SIZE=20
DB_MAX_OVERFLOW=40
DB_POOL_TIMEOUT=30

# --- Cache Redis (opcional: deixe vazio para desabilitar) ---
REDIS_URL=
CACHE_TTL_SECONDS=86400
BATCH_MAX_SIZE=1000
